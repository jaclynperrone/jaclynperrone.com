---
title: Gallery
---
<section class="site-content site-content--about">
  <div class="gallery">
    <nav class="gallery__nav">
      <ul class="gallery-nav-list">
        <li class="gallery-nav-list__item gallery-nav-list__item--prev js-gallery-prev">
          ⟵ prev
        </li>
        <li class="gallery-nav-list__item gallery-nav-list__item--next js-gallery-next">
          next ⟶
        </li>
      </ul>
    </nav>

    <ul class="gallery-list">
      <% data.gallery.images.each do |i| %>
        <li class="gallery-list__item js-gallery-item"><%=image_tag "#{i}", class: "gallery-list__image" %></li>
      <% end %>
    </ul>
  </div>
</section>
